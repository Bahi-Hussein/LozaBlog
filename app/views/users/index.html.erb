<h1> All Users </h1>

<% @users.all.each do |user| %>
	<h3> <%= user.name %></h3>
	<p> <%= user.desc %> </p>
	<p> <%= link_to "show", user_path(user) %> - <%= link_to "Edit", edit_user_path(user) %> -
	<%= link_to "Delete", user, method: :delete %>
<% end %>

<p><%= link_to "home", root_path %></p>
